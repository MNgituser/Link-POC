[access]
	inheritFrom = All-Projects
[project]
	description = this is a sample project for POC
[access "refs/*"]
	owner = group user/MNgituser
[access "refs/for/refs/*"]
	read = group user/MNgituser
	push = group user/MNgituser
	label-Code-Review = -2..+2 group user/MNgituser
	label-Verified = -1..+1 group user/MNgituser
	rebase = group user/MNgituser
	removeReviewer = group user/MNgituser
	submit = group user/MNgituser
[access "refs/heads/*"]
	read = group user/MNgituser
	create = group user/MNgituser
	push = +force group user/MNgituser
	pushMerge = group user/MNgituser
[access "refs/tags/*"]
	push = group user/MNgituser
	push = +force group user/MNgituser
